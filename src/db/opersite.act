create table t_opersite
(
  operid number(9),
  siteno integer,
  lastsaved varchar(30)
)
begin
action modename ykt_cur end
action add end
action read_by operid siteno end
action update_by operid siteno end
action del_by operid siteno end
action read_lock_by c0 operid siteno end
action update_lock_by c0 end
action free_lock_by c0 end
end
