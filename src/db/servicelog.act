create table t_servicelog
(
  settledate number,
  servicename varchar2(60),
  starttime varchar2(14)
)
begin
action modename ykt_cur end
action add end
action read_by settledate servicename end
action update_by settledate servicename end
action del_by settledate servicename end
action read_lock_by c0 settledate servicename end
action update_lock_by c0 end
action del_lock_by c0 end
action free_lock_by c0 end
end

